pipeline {
    agent any
    tools {
	  maven 'Maven 3.9.6'
	}
    stages {
        stage('git clone') {
            steps {
                git branch: 'v1', credentialsId: '371eb9e8-9b5b-443a-b0d6-fe46cb107d71', url: 'https://github.com/keerthana962024/kprepo.git'
            }
			}
        stage('list') {
            steps {
                sh 'ls -l'
            }
			}
        stage('validate') {
            steps {
                sh 'mvn validate'
            }
			}
		stage('compile') {
            steps {
                sh 'mvn compile'
            }
			}
		stage('test') {
            steps {
                sh 'mvn test'
            }
			}
			
        }
    }
